{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/listing/page.tsx.bac",
    "src/app/listing/[id]/page.tsx,bac",
    "src/app/listing/[id]/page.tsx.bac"
, "src/pages/PropertyDetails.tsx.bac", "src/pages/Properties.tsx.bac", "src/lib/mongodb.js"  ],
  "exclude": [
    "node_modules"
  ]
}
